{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": 0,
        "semi": 0,
        "quotes": [
            "error",
            "single"
        ]
    }
}
